    "wget", "curl", "nc", "netcat", "python", "perl", "ruby",
    "bash", "sh", "powershell", "cmd", "sudo", "su", "passwd",
    "cat /etc/passwd", "cat /etc/shadow", "whoami", "id",
    "uname", "ps", "netstat", "ss", "lsof", "nmap", "masscan",
    "rm -rf", "chmod", "chown", "kill", "killall", "pkill",
    "iptables", "firewall", "systemctl", "service", "crontab",
    "history", "last", "w", "who", "uptime", "df", "mount",
    "tar", "zip", "unzip", "gunzip", "ssh", "scp", "rsync"
  ],
  "max_message_length": 1900,
  "rate_limit_delay": 1,
  "high_priority_ips": [],
  "ignored_ips": [],
  "notification_channels": {
    "general": "REPLACE_WITH_GENERAL_WEBHOOK_URL",
